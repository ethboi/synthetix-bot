    "eslint-plugin-prettier": "^4.2.1",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "prettier": "^2.8.3"
  },
  "dependencies": {
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "rimraf": "^4.3.1",
    "twitter-api-v2": "^1.14.2",
