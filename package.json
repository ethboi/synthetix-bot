{
  "name": "synthetix-bot",
  "version": "1.0.0",
  "description": "Synthetix Bot",
  "main": "index.js",
  "repository": "git@github.com:ethboi/synthetix-bot.git",
  "author": "ethboi",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --files --respawn src/app.ts",
    "clean": "rimraf ./build",
    "build": "yarn clean && npx tsc",
    "start": "node build/app.js",
    "generate-types": "yarn typechain --target ethers-v5 --out-dir src/contracts/typechain src/contracts/abis/*.json",
    "deploycmds": "tsc && node ./deploy-commands.js",
    "compile-synthetix": "graphql-codegen --config ./config/codegen-synthetix.ts"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/ethereum-block-by-date": "^1.4.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5",
    "yarn-upgrade-all": "^0.7.2"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@graphprotocol/graph-ts": "^0.30.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@synthetixio/wei": "^2.74.4",
    "axios": "^1.3.6",
    "cross-fetch": "^3.1.5",
    "discord.js": "^14.15.2",
    "dotenv": "^16.0.3",
    "ethereum-block-by-date": "^1.4.7",
    "ethers": "^5.7.2",
    "graphql": "^16.6.0",
    "lodash": "^4.17.21",
    "node-schedule": "^2.1.1",
    "rimraf": "^4.3.1",
    "telegraf": "^4.12.2",
    "ts-node": "^10.5.0",
    "twitter-api-v2": "^1.14.2",
    "typechain": "^8.3.2"
  }
}
